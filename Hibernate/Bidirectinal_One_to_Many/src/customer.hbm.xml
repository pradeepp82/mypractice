<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"Xmlfiles/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 <class name="Com.Artek.Customer" table="customer">
 
 
 <id name="customerid">
  <generator class="increment"></generator>
 </id>
 <property name="customername"></property>
  <property name="forevenid"  insert="false" update="false"></property> 
 
 <many-to-one  name="parentobject" column="forevenid"  class="Com.Artek.Vendor"    cascade="all"/> 
 
 
 
 
 </class>


</hibernate-mapping>